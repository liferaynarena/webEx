<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.2.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_6_2_0.dtd">

<routes>
	<route>
		<pattern>/{siteDetails}/{siteId}</pattern>
		<generated-parameter name="javax.portlet.action">{siteDetails}</generated-parameter>
		<generated-parameter name="siteId">{siteId}</generated-parameter>
		<implicit-parameter name="p_p_id">sitetemplatessearch_WAR_sitetemplatessearchportlet</implicit-parameter>
	    <implicit-parameter name="p_p_lifecycle">1</implicit-parameter>
		<implicit-parameter name="p_p_state">normal</implicit-parameter>
		<implicit-parameter name="p_p_mode">view</implicit-parameter>
		<implicit-parameter name="p_p_col_id">column-1</implicit-parameter>
        <implicit-parameter name="p_p_col_count">1</implicit-parameter>
	</route>
	
	<route>
		<pattern>/create-site</pattern>
		<implicit-parameter name="p_p_id">sitetemplatessearch_WAR_sitetemplatessearchportlet</implicit-parameter>
		<implicit-parameter name="javax.portlet.action">createSite</implicit-parameter>
	    <implicit-parameter name="p_p_lifecycle">1</implicit-parameter>
		<implicit-parameter name="p_p_state">normal</implicit-parameter>
		<implicit-parameter name="p_p_mode">view</implicit-parameter>
		<implicit-parameter name="p_p_col_id">column-1</implicit-parameter>
        <implicit-parameter name="p_p_col_count">1</implicit-parameter>
	</route>
	
	<route>
		<pattern>/enter-site-details</pattern>
		<implicit-parameter name="p_p_id">sitetemplatessearch_WAR_sitetemplatessearchportlet</implicit-parameter>
		<implicit-parameter name="javax.portlet.action">enterSiteDetails</implicit-parameter>
	    <implicit-parameter name="p_p_lifecycle">1</implicit-parameter>
		<implicit-parameter name="p_p_state">normal</implicit-parameter>
		<implicit-parameter name="p_p_mode">view</implicit-parameter>
		<implicit-parameter name="p_p_col_id">column-1</implicit-parameter>
        <implicit-parameter name="p_p_col_count">1</implicit-parameter>
	</route>
	
	<route>
		<pattern>/{urlTitle}</pattern>
		<generated-parameter name="urlTitle">{urlTitle}</generated-parameter>
		<implicit-parameter name="p_p_id">sitetemplatessearch_WAR_sitetemplatessearchportlet</implicit-parameter>
		<implicit-parameter name="javax.portlet.action">viewSites</implicit-parameter>
	    <implicit-parameter name="p_p_lifecycle">1</implicit-parameter>
		<implicit-parameter name="p_p_state">normal</implicit-parameter>
		<implicit-parameter name="p_p_mode">view</implicit-parameter>
		<implicit-parameter name="p_p_col_id">column-1</implicit-parameter>
        <implicit-parameter name="p_p_col_count">1</implicit-parameter>
	</route>
	
	
	

</routes>

